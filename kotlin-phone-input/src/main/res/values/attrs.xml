<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PhoneInputLayout">
        <attr name="pcv_icon_size" format="dimension" />
        <attr name="pcv_custom_flag_shape" format="string" />

        <attr name="pcv_flag_shape" format="enum">
            <enum name="hart" value="0" />
            <enum name="circle" value="1" />
            <enum name="circle_airplane" value="2" />
            <enum name="circle_pin" value="3" />
            <enum name="square" value="4" />
            <enum name="square_round" value="5" />
            <enum name="rhombus" value="6" />
            <enum name="triangle" value="7" />
            <enum name="chat" value="8" />
            <enum name="chat_2" value="9" />
            <enum name="pin" value="10" />
            <enum name="pin_big" value="11" />
            <enum name="cluster_pin" value="12" />
            <enum name="star" value="13" />
            <enum name="star_round" value="14" />
            <enum name="house" value="15" />
            <enum name="house_2" value="16" />
            <enum name="compass_ahead" value="17" />
            <enum name="compass_top_right" value="18" />
            <enum name="backward" value="19" />
            <enum name="man" value="20" />
            <enum name="email" value="21" />
            <enum name="cloud" value="22" />
            <enum name="phone" value="23" />
            <enum name="play" value="24" />
            <enum name="oil" value="25" />
        </attr>

        <attr name="pcv_icon_position" format="enum">
            <enum name="start" value="0" />
            <enum name="end" value="1" />
        </attr>
    </declare-styleable>
</resources>